<Window x:Class="rpi_gpio_client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:rpi_gpio_client"
        mc:Ignorable="d"
        Title="RPI GPIO CLIENT" Height="450" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Left" Height="409" VerticalAlignment="Top" Width="782">
            <Button x:Name="CallApiBtn" Content="Call Api" ClickMode="Press" Click="CallApiBtn_Click"/>
            <TextBlock x:Name="CallApiOutput" TextWrapping="Wrap" Height="389"/>
        </StackPanel>

    </Grid>
</Window>
